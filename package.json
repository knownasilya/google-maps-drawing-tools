    "shelljs": "^0.7.8",
    "standard-version": "^4.3.0",
    "tslint": "^5.7.0",
    "typedoc": "^0.10.0",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "ts-std": "^0.6.2"
