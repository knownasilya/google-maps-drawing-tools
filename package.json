{
  "name": "google-maps-drawing-tools",
  "version": "0.0.0",
  "description": "A brand new TypeScript library.",
  "main": "dist/commonjs/src/index.js",
  "module": "dist/modules/src/index.js",
  "types": "dist/types/src/index.d.ts",
  "ember-addon": {
    "main": "ember-addon.js"
  },
  "keywords": [
    "ember-addon"
